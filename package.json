{
  "name": "pgats-2025-02-base",
  "version": "1.0.0",
  "description": "Trabalho de conclusão da disciplina de Automação de Testes em APIs",
  "main": "index.js",
  "scripts": {
    "test": "mocha \"test/**/*.test.js\" --timeout 10000 --reporter mochawesome --reporter-options quiet=true",
    "test-graphql": "npm run test-graphql-controller && npm run test-graphql-external",
    "test-graphql-controller": "mocha \"test/graphql/controller/**/*.test.js\" --timeout 10000 --reporter mochawesome --reporter-options quiet=true",
    "test-graphql-external": "mocha \"test/graphql/external/**/*.test.js\" --timeout 10000 --reporter mochawesome --reporter-options quiet=true",
    "test-rest": "npm run test-rest-controller && npm run test-rest-external",
    "test-rest-controller": "mocha \"test/rest/controller/**/*.test.js\" --timeout 10000 --reporter mochawesome --reporter-options quiet=true",
    "test-rest-external": "mocha \"test/rest/external/**/*.test.js\" --timeout 10000 --reporter mochawesome --reporter-options quiet=true",
    "start-graphql": "node graphql/server.js",
    "start-rest": "node rest/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juliodelimas/pgats-2025-02-base.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/juliodelimas/pgats-2025-02-base/issues"
  },
  "homepage": "https://github.com/juliodelimas/pgats-2025-02-base#readme",
  "dependencies": {
    "apollo-server-express": "^3.13.0",
    "express": "^4.21.2",
    "graphql": "^16.11.0",
    "jsonwebtoken": "^9.0.2",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "chai": "^6.0.1",
    "chai-exclude": "^3.0.1",
    "dotenv": "^17.2.2",
    "mocha": "^11.7.2",
    "mochawesome": "^7.1.3",
    "sinon": "^21.0.0",
    "supertest": "^7.1.4"
  }
}
